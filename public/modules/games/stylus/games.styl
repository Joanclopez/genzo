.Character
  margin-top 2em
  overflow hidden
  &-image
    width 200px
    height 200px
  &-name
    margin 1em
  &-NameText
    text-align center
    font-size 2em
    color blue
.BattleRoom
  display block
  margin-top 2em
  background-color white
  overflow hidden
  &-userInformation
    overflow hidden
    border-bottom 1px solid black
  &-avatarImg
    width 50px
    height 50px

.PanelInformation
  text-align center
  &-characterImage
    margin 2em 0
  &-img
    width 200px
    height 200px
.PanelBattle
  &-attack
    margin 13em
  &-text
    font-size 5em
