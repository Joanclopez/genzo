.Character
  margin-top 2em
  overflow hidden
  &-image
    width 200px
    height 200px
  &-name
    margin 1em
  &-NameText
    text-align center
    font-size 2em
    color blue
.BattleRoom
  display block
  margin-top 2em
  background-color white
  overflow hidden
  &-userInformation
    overflow hidden
    border-bottom 1px solid black
  &-avatarImg
    width 50px
    height 50px

.PanelInformation
  text-align center
  &-characterImage
    margin 2em 0
  &-img
    width 200px
    height 200px
.PanelBattle
  &-attack
    margin 13em
  &-text
    font-size 5em

.Fight-content
  width: 100%
  height: 100vh
  background-color #000
  .spriteP1
    width 122px
    height 142px
    //background-image url('../modules/games/img/goku1.png')
    background-image url('../img/goku1.png')
  #north
    margin 0 auto
    transform rotate(180deg)
  .central
    #west
      float right
      transform rotate(90deg)
    #east
      float left
      transform rotate(-90deg)
  #south
    margin 0 auto
